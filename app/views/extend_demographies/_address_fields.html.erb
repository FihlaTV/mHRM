<div class="nested-fields row">
  <fieldset>

    <div class="col-md-12">
      <%= link_to_remove_association "remove", f, class: 'btn btn-warning pull-right' %>

      <div class="form-group col-md-12">
        <%= f.label :address_type , class: 'col-md-2 control-label' %>
        <div class="col-md-8">
          <%= f.select :address_type, AddressType.active.pluck(:name, :id), {}, {class: 'form-control'} %>
        </div>
      </div>

      <div class="form-group col-md-12">
        <%= f.label :address , class: 'col-md-2 control-label' %>
        <div class="col-md-8">
          <%= f.text_field :address , class: 'form-control' %>
        </div>
      </div>

      <div class="form-group col-md-12">
        <%= f.label :city , class: 'col-md-2 control-label' %>
        <div class="col-md-8">
          <%= f.text_field :city, class: 'form-control' %>
        </div>
      </div>

      <div class="form-group col-md-12">
        <%= f.label :zip_code , class: 'col-md-2 control-label' %>
        <div class="col-md-8">
          <%= f.text_field :zip_code , class: 'form-control' %>
        </div>
      </div>

      <div class="form-group col-md-12">
        <%= f.label :state , class: 'col-md-2 control-label' %>
        <div class="col-md-8">
          <%= f.text_field :state , class: 'form-control' %>
        </div>
      </div>

      <div class="form-group col-md-12">
        <%= f.label :country_code, class: 'col-md-2 control-label' %>
        <div class="col-md-8">
          <%= f.text_field :country_code, class: 'form-control' %>
        </div>
      </div>

    </div>

  </fieldset>
</div>
