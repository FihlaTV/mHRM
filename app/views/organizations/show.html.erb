<%= link_to 'Delete',  organization_path(@organization), class: 'btn btn-danger pull-right',
    :method => :delete,
    :data => {:confirm => t(:text_are_you_sure)}
     %>
<%= link_to 'Edit', edit_organization_path(@organization), class: 'btn btn-success pull-right' %>
<div class="col-xs-12">
  <p>
    <strong>Name:</strong>
    <%= @organization.name %>
  </p>

  <p>
    <strong>Note:</strong>
    <%= @organization.note %>
  </p>

  <%= render_extend_demography_information(@organization.organization_extend_demography) %>
</div>